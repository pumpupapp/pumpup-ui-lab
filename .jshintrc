{
  // Enforcing
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "forin": true,
  "freeze": true,
  "immed": true,
  "indent": 2,
  "latedef": "nofunc",
  "maxcomplexity": 6,
  "maxdepth": 3,
  "maxlen": 120,
  "maxparams": 4,
  "maxstatements": 20,
  "newcap": true,
  "noarg": true,
  "noempty": true,
  "nonbsp": true,
  "quotmark": "single",
  "strict": true,
  "undef": true,
  "unused": "vars",

  // Relaxing
  "asi": true,
  "eqnull": true,
  "esnext": true,
  "noyield": true,

  // Environments
  "browser": true,
  "devel": true
}